package com.example.accessingdatamysql;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
/**
 * Questa classe rappresenta un utente nel sistema.
 * 
 * Ogni istanza di questa classe rappresenta un utente con un ID univoco generato automaticamente,
 * un nome e un numero di telefono.
 * 
 * Questa classe Ã¨ stata annotata con @Entity per indicare a Hibernate di creare una tabella
 * nel database relazionale corrispondente a questa classe.
 * 
 * @author Giuseppe Crimi
 */
@Entity // This tells Hibernate to make a table out of this class
public class User {
	@Id
	@GeneratedValue(strategy = GenerationType.AUTO)
	private Integer id;

	private String name;

	private long numeroTelefono;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public long getNumeroTelefono() {
		return numeroTelefono;
	}

	public void setNumeroTelefono(long numeroTelefono) {
		this.numeroTelefono= numeroTelefono;
	}
}